{
  "name": "BodegaBox",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "boxId": {
      "type": "string"
    },
    "location": {
      "type": "geopoint"
    },
    "itemLimit": {
      "type": "number",
      "default": 10
    },
    "minItemTotalCountLimit": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "organisation": {
      "type": "belongsTo",
      "model": "Organisation",
      "foreignKey": "organisationId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
